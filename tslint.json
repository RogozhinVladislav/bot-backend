{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-config-airbnb"],
    "linterOptions": {
        "exclude": ["node_modules/**/*.ts", "dist/**/*.ts"]
    },
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "semicolon": false,
        "quotemark": [true, "single"],
        "ordered-imports": false,
        "ter-arrow-parens": false,
        "import-name": false
    },
    "rulesDirectory": []
}